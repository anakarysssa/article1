    <script>
        function highlightSignificant() {
            const cells = document.querySelectorAll('td');
            cells.forEach(cell => {
                const text = cell.textContent.trim();
                if (text.includes('<0.001') || text.includes('< 0.001')) {
                    cell.classList.add('p-value-highly-significant');
                } else if (text.includes('<0.05') || text.includes('< 0.05') || 
                          (parseFloat(text) < 0.05 && !isNaN(parseFloat(text)) && text !== '')) {
                    cell.classList.add('p-value-significant');
                }
            });
        }
        
        function resetHighlight() {
            const cells = document.querySelectorAll('td');
            cells.forEach(cell => {
                cell.classList.remove('p-value-significant', 'p-value-highly-significant');
            });
        }
        
        function toggleUnknownRows() {
            const unknownRows = document.querySelectorAll('.unknown-row');
            unknownRows.forEach(row => {
                row.style.display = row.style.display === 'none' ? '' : 'none';
            });
        }
        
        // Auto-highlight on load
        document.addEventListener('DOMContentLoaded', function() {
            highlightSignificant();
        });
    </script>
</body>
